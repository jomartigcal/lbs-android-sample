<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.tigcal.samples.billcalc">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_bc_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_bc_launcher"
        android:supportsRtl="true">
        <activity
            android:name=".BillCalcActivity"
            android:icon="@mipmap/ic_bc_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_bc_launcher_round"
            android:taskAffinity="com.tigcal.samples.billcalc"
            android:theme="@style/AppTheme.BillCalc.LaunchScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>